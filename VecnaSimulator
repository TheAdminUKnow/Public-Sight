local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()
local PhantomForcesWindow = Library:NewWindow("NTM | Vecna Simulator")


local KillingCheats = PhantomForcesWindow:NewSection("Main Menu")


KillingCheats:CreateToggle("Kill Server", function(tt)
if tt then

local Tool = game:GetService("Players").LocalPlayer.Backpack.Atack
if Tool ~= nil then
Tool.Parent = game.Players.LocalPlayer.Character
end


 _G.body = true ---- make true into false to stop loop
while _G.body == true do wait() 
pcall(function() 
  

for i,v in pairs(game.Players:GetPlayers()) do
  
if v.Name ~= game.Players.LocalPlayer.Name then

local args = {
    [1] = v.Character.Humanoid,
    [3] = v.Character.Torso
}

game:GetService("Players").LocalPlayer.Character.Atack.LocalScript.punched:InvokeServer(unpack(args))

  
end
end
 
end)
end
else

 _G.body = false ---- make true into false to stop loop
while _G.body == true do wait() 
pcall(function() 
 -- Script generated by Nut The Menace Spy - credits to Claudev0#8849

local args = {
    [1] = game:GetService("Players").neptunekittens.Character.Humanoid,
    [3] = game:GetService("Players").neptunekittens.Character.Torso
}

game:GetService("Players").LocalPlayer.Character.Atack.LocalScript.punched:InvokeServer(unpack(args))
  
end)
end
end
end)






local Farming = PhantomForcesWindow:NewSection("Farm Currency")

Farming:CreateToggle("All Chest TP", function(state)


if state then

 _G.farmr = true ---- make true into false to stop loop
while _G.farmr == true do wait()
local args = {
    [1] = game:GetService("Players").LocalPlayer
}

game:GetService("ReplicatedStorage").events.train:FireServer(unpack(args))

end

else                   

 _G.farmr = false ---- make true into false to stop loop
while _G.farmr == true do wait()
local args = {
    [1] = game:GetService("Players").LocalPlayer
}

game:GetService("ReplicatedStorage").events.train:FireServer(unpack(args))

end


end 
end)



game.StarterGui:SetCore("SendNotification", { 
            Title = "Nut The Menace <3";
            Text = "Heroes: Online World GUI                       ~~~~ Now Active ~~~~";
            Icon = "rbxthumb://type=Asset&id=7770638479&w=150&h=150";
            Duration = "30";
            Button1 = "Enjoy :)";
        })
    

